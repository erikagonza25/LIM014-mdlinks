{
    "env": {
       "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"

    },
    "rules": {
      "no-console": "warn",
      "linebreak-style": 0,
      "prefer-template":"off",
      "no-else-return":"off",
      "no-confusing-arrow":"off",
      "no-restricted-syntax": 0,
      "implicit-arrow-linebreak":0
    }
}
